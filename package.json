{
  "name": "vite-vue",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build --mode=production",
    "dev": "vite --mode=development",
    "lint": "npm run lint:eslint && npm run lint:tsc && npm run lint:vuets",
    "lint:eslint": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/",
    "lint:eslint:quiet": "eslint --quiet \"{tests,src}/**/*.{js,ts,tsx,vue}\"",
    "lint:fix": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/ --fix",
    "lint:vuets": "vue-tsc --noEmit",
    "lint:tsc": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "type-fest": "^3.5.7",
    "vite": "^4.1.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/test-utils": "^2.2.10",
    "eslint": "^8.34.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.9.0",
    "lint-staged": "^13.1.1",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4",
    "vue-tsc": "^1.0.24"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
