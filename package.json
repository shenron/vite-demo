{
  "name": "vuejsglobal",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:tsc": "eslint --quiet \"{tests,src}/**/*.{js,ts,tsx,vue}\" && npm run check-types",
    "check-types": "tsc --noemit --skipLibCheck",
    "lint:eslint": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/",
    "lint:fix": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/ --fix",
    "test": "jest"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^1.2.2",
    "@vitejs/plugin-vue-jsx": "^1.1.4",
    "@vue/test-utils": "^2.0.0-rc.6",
    "vite": "^2.3.2",
    "vue": "^3.0.2",
    "vue-router": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-transform-flow-comments": "^7.12.13",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-router": "^5.0.0-alpha.8",
    "@vue/cli-plugin-unit-jest": "^5.0.0-beta.1",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-vue": "^7.9.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "vue-eslint-parser": "^7.6.0",
    "vue-jest": "^5.0.0-alpha.9"
  }
}
