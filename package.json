{
  "name": "vite-vue",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build --mode=production",
    "dev": "vite --mode=development",
    "lint": "npm run lint:eslint && npm run lint:tsc && npm run lint:vuets",
    "lint:eslint": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/",
    "lint:eslint:quiet": "eslint --quiet \"{tests,src}/**/*.{js,ts,tsx,vue}\"",
    "lint:fix": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/ --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:vuets": "vue-tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
